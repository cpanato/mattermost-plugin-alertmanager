{
    "id": "alertmanager",
    "name": "AlertManager",
    "description": "Alermanager plugin for Mattermost, you can receive alerts and interact with alertmanager.",
    "homepage_url": "https://github.com/cpanato/mattermost-plugin-alertmanager",
    "support_url": "https://github.com/cpanato/mattermost-plugin-alertmanager/issues",
    "release_notes_url": "https://github.com/cpanato/mattermost-plugin-alertmanager/releases/tag/v0.4.0",
    "icon_path": "assets/alertmanager-logo.svg",
    "version": "0.4.0",
    "min_server_version": "7.5.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "settings": [{
            "key": "alertConfigs",
            "type": "custom",
            "display_name": "Alert manager settings:"
        }]
    }
}
